{
    "name": "marvinklemp/money",
    "description": "A lightweight money library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Klemp",
            "email": "mail@marvin-klemp.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "MarvinKlemp\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarvinKlemp\\Money\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
